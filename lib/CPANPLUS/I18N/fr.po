msgstr "%1 -- Exploration et installation de modules CPAN (v%2)"

#: lib/CPANPLUS/Dist/PPM.pm:523
#. ($item)
msgid "%1 could not be added by Archive::Zip"
msgstr "%1 ne peut être ajouté par Archive::Zip"

#: lib/CPANPLUS/Internals.pm:710
#. ($mod)
msgid "%1 does not contain an author directory!"
msgstr "%1 ne contient pas de repertoire d'auteurs"

#: lib/CPANPLUS/Dist/PPM.pm:280
